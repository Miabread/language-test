struct Unit();
union Never();

union Boolean(True, False) {

    func not(self: Self) -> Self {
        match(self) {
            _::True -> _::False,
            _::False -> _::True,
        }
    }
}

/*
 *  Optional::Some(123).unwrap(or: 12)
 */
union Optional<T>(
    Some(T),
    Nothing,
) {
    func unwrap(self: Self, or default: T) -> T {
        match(self) {
            _::Some(value) -> value,
            _::Nothing -> default,
        }
    }
}
